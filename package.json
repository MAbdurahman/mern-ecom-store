{
  "name": "mern-ecom-store",
  "version": "1.0.0",
  "description": "E-commerce store utilizing MERN Stack",
  "main": "backend/server.js",
  "repository": "https://github.com/MAbdurahman/mern-ecom-store.git",
  "author": "Mahdi Abdurrahman <mdbdrrhm@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node backend/server.js",
    "server": "SET NODE_ENV=development& nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prod": "SET NODE_ENV=production& node backend/server"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.5.1",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.10.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "paypal-rest-sdk": "^1.8.1"
  },
  "nodemonConfig": {
    "delay": 2000
  }
}